program {
  a, i, divider ~ integer
  divisible ~ boolean

  read(a) 
  
  i <- 2
  divisible <- false
  while not divisible {
    if not divisible and a mod i = 0 {
      divisible <- true
      divider <- i
    }
    i <- i + 1
  }
  if divisible {
    write(divisible)
    write(divider)
  } else {
    write(divisible)
  }
}